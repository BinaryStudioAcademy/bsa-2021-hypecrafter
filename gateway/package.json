    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "rand-token": "^1.0.1",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
