    "migration-revert": "ts-node node_modules/typeorm/cli.js migration:revert",
    "seed": "ts-node ./src/data/seeders",
    "typeorm": "ts-node node_modules/typeorm/cli.js"
  },
  "author": "",
  "license": "ISC",
