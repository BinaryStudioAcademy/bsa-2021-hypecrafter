    "@types/lodash": "^4.14.171",
    "@types/node": "16.0.0",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "cross-env": "^7.0.3",
    "nodemon": "2.0.9",
    "ts-node": "^10.1.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
